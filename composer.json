{
    "name": "andatech/yii2-cms",
    "description": "Yii 2 CMS from Anda Tech",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/andatech/yii2-cms.git",
        "issues": "https://github.com/andatech/yii2-cms/issues"
    },
    "authors": [
        {
            "name": "Anda Technology",
            "email": "kuakling@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "andatech/yii2-user":"dev-master",
        "2amigos/yii2-ckeditor-widget": "^2.0",
        "sunhater/kcfinder": "dev-master",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-export": "*",
        "kartik-v/yii2-mpdf": "*",
        "kartik-v/yii2-detail-view": "*",
        "maxmirazh33/yii2-uploadable-cropable-image": "*",
        "wowkaster/yii2-serialize-attributes": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "anda\\cms\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}

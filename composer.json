{
    "name": "andatech/yii2-cms",
    "description": "Yii 2 CMS from Anda Tech",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/andatech/yii2-cms.git",
        "issues": "https://github.com/andatech/yii2-cms/issues"
    },
    "authors": [
        {
            "name": "Anda Technology",
            "email": "kuakling@gmail.com",
            "role" : "Developer"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/andatech/yii2-cms.git",
            "branch": "master"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "andatech/yii2-user":"dev-master"
    },
    "autoload": {
        "psr-4": {
            "anda\\cms\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
